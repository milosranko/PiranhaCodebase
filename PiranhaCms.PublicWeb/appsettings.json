{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/application.log",
          "rollingInterval": "Hour"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "piranha": ""
  }
}
